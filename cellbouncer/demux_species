#preping k-mer library
utils/demux_species_ref.py -k 60 -o [path to storing k-mers library] \
    -n Human Chimp Gorilla Macaque Mouse \
    -f [species1_fasta] [species2_fasta] ... \
    (-g [species1_gtf] [species2_gtf] ... ) \
    -N -1 \
    (--FastK /path/to/utils/demux_species_ref.py)

